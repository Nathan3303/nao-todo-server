{
  "name": "nao-todo-server",
  "version": "0.1.0",
  "description": "NaoTodoServer is a server for NaoTodo based on NestJS framework.",
  "packageManager": "pnpm@9.12.1",
  "author": "Nathan Lee",
  "license": "MIT",
  "main": "dist/src/main",
  "scripts": {
    "lint": "npx eslint -c eslint.config.mjs",
    "lint-on-debug": "npx eslint -c eslint.config.mjs --debug",
    "lint:lint-staged": "lint-staged",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node ."
  },
  "dependencies": {
    "@nestjs/common": "^11.0.9",
    "@nestjs/core": "^11.0.9",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/platform-express": "^11.0.9",
    "mongoose": "^8.10.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.0.3",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  }
}